<%- include('header.ejs') %>

<div class="container-fluid">
	<div class="row">
		<div class="col-md-3"></div>
		<div class="col-md-6 md-3 p-3">
			<table id="pp" class="table table-bordered">
				<tr>
					<th class="text-center" colspan="2">Appointment Details</th>
				</tr>
				<tr>
					<th>Reference no.</th>
					<td><%= appointment.ref %></td>
				</tr>
				<tr>
					<th>Name</th>
					<td><%= appointment.name %></td>
				</tr>
				<tr>
					<th>Email</th>
					<td><%= appointment.email %></td>
				</tr>
				<tr>
					<th>DOB</th>
					<td><%= appointment.dob %></td>
				</tr>
				<tr>
					<th>Sex</th>
					<td><%= appointment.sex %></td>
				</tr>
				<tr>
					<th>Date</th>
					<td><%= appointment.date %></td>
				</tr>
				<tr>
					<th>Specialities</th>
					<td><%= appointment.specialities %></td>
				</tr>
				<tr>
					<th>Moblie</td>
					<td><%= appointment.phone %></td>
				</tr>
				<tr>
					<th>Address</th>
					<td><%= appointment.address %></td>
				</tr>
				<tr>
					<th>Cities</th>
					<td><%= appointment.cities %></td>
				</tr>
				<tr>
					<td class="text-center" colspan="2"><input type="button" value="Print" onclick="window.print(document.getElementById(pp))"></td>
				</tr>
			</table>
		</div>
		<div class="col-md-3"></div>
	</div>
</div>

<%- include('footer.ejs') %>